<h1> Hey {{name}}, die Schritte der letzten Tage aller User</h1>
<table class="table">
	<thead>
		<tr>
			<th scope="col">
				Username
			</th>
			<th scope="col">
				Schritte
			</th>
		</tr>
	</thead>
	<tbody>
		{{#each users}}
		
		{{#if user}}
			<tr class="table-secondary">
				<td>
					{{username}}
				</td>
				<td>
					{{total}}
				</td>
			</tr>
		{{else}}
			<tr>
				<td>
					{{username}}
				</td>
				<td>
					{{total}}
				</td>
			</tr>
		{{/if}}
		{{/each}}
	</tbody>
</table>